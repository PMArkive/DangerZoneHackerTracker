<controls:BiaWindow x:Class="DangerZoneHackerTracker.MissingParamsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DangerZoneHackerTracker"
        xmlns:controls="clr-namespace:Biaui.Controls;assembly=Biaui"
        mc:Ignorable="d"
        Style="{StaticResource BiaWindowSpaceSavingStyle}"
        Title="Missing Parameters" Height="450" Width="800">
    <Grid>
        <TextBox Background="Transparent" Foreground="AliceBlue" Height="48"
         BorderThickness="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Margin="0,-128,0,0"
         Text="Missing '-netconport 2121' from csgo's launch options."
         IsReadOnly="True" TextWrapping="Wrap" />
        <TextBox Background="Transparent" Foreground="AliceBlue" Height="48"
         BorderThickness="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Margin="0,0,0,0"
         Text="Application will not function properly without it."
         IsReadOnly="True" TextWrapping="Wrap" />
    </Grid>
</controls:BiaWindow>
